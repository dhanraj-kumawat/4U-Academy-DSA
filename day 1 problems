public class question2{
    public static void main(String[] args) {
        int arr[] = {10,2,50,8,12,9,3};
      for(int i= 0; i<arr.length; i++){
        if(arr[i] %2 == 0)
        System.out.println(arr[i]);
      }
    }
}

