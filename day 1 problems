#question 22

import java.util.*;

public class question22 {
    public static void main(String[] args) {
        int a[] = { 1, 2, 3, 4, 5, 6, 7 };
        int sum = 0;
        Scanner sc = new Scanner(System.in);
        System.out.println("enter ith index: ");
        int i = sc.nextInt();

        for (int j = 0; j <= i; j++) {
            sum += a[j];

        }
        System.out.println("the sum is:  "+sum);
    }
}


