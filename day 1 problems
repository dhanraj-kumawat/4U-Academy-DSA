#question 6

public class question6 {
    public static void main(String[] args) {
        int arr[] = {1,2,3,7,19,20,2,13,25};
        int k = 13;

        for(int i = 0; i<arr.length; i++){
            if(arr[i]== k){
                System.out.println(i);
                System.exit(0);
            }
        }
        System.out.println("-1");
    }
}


#quesiton 7 

public class question7 {
    public static void main(String[] args) {
        int arr[] = {1,3,5,8,9};
          int temp = arr[0];
          for(int i= 0; i<arr.length -1; i++){
            arr[i]=arr[i+1];
          }
          arr[arr.length-1] = temp;

          for(int j = 0; j<arr.length; j++){
            System.out.println(arr[j]);
          }
    }
}


#question 8

public class queston8 {
    public static void main(String[] args) {
        int xyz [] = {1,1,1,0,1,0,1,0,0,1,1,1,0,1,0,1,1};
        int zeroCount = 0, oneCount = 0;
        for(int i = 0; i<xyz.length; i++){
          if(xyz[i]== 0)
          zeroCount++;
          if(xyz[i]== 1)
          oneCount++;

        }
        System.out.println("number of zeros : "+ zeroCount);
        System.out.println("number of ones : "+ oneCount);
    }
}


#question 9

public class question9{
    public static void main(String[] args) {
        int arr[][]= {{2,4,6},
                      {3,5,7},   
                      {1,2,3}};

        for(int i = 0; i<arr[i].length;i++){
            int rowSum = 0;
            for(int j = 0; j<arr[i].length; j++)
            {
                rowSum+=arr[i][j];
            }
            System.out.println("the sum of row " + (i+1) +" is : "+ rowSum);
        }
    }
}


#question 10

public class question10 {
    public static void main(String[] args) {
        int arr[] = {1,2,3,4,2,3,7,4};

        for(int i = 0; i<arr.length; i++){
            for(int j= i+1; j<arr.length; j++){
                if (arr[i]==arr[j]){
                    System.out.println(arr[i]);
                
                }
            }
        }

    }
}



#question 11


#question 16 

public class question16 {
    public static void main(String[] args) {
        int a[][] = { { 1, 2, 3 },
                { 3, 4, 5 } };

        int b[][] = { { 2, 3, 4 },
                { 4, 5, 6 },
                { 1, 2, 3 } };

        int c[][] = new int[a.length][b[0].length];

        for (int i = 0; i < a.length; i++) {
            for (int j = 0; j < b[0].length; j++) {
                for (int k = 0; k < a[0].length; k++) {

                    c[i][j] += a[i][k] * b[k][j];
                }
            }

        }
        for(int i=0;i<c[0].length;i++){  
            for(int j=0;j<c[0].length;j++){  
              System.out.print(c[i][j]+" ");  
            }  
            System.out.println();  
           } 
    }
}

#question 17

public class question17 {
    public static void main(String[] args) {
        int arr[] = { 1, 2, 3, 4, 5, 6, 0 };
        int k = 9;
        int i = 4;

        for (int j = arr.length - 1; j > i; j--) {
            arr[j] = arr[j - 1];
        }
        arr[i] = 9;

        for (int x : arr) {
            System.out.println(x);
        }
    }
}



#question 18

public class question18 {
    public static void main(String[] args) {
        int a[] = {1,2,3,4,5,6,7};
        int b[] = {11,33,66,77,99,45,22};

        int c[] = new int [7];

        for(int i = 0; i<7; i++){
            c[i] = a[i]+b[i];
        }
         
        for (int x : c) {
            System.out.println(x);
        }
    }
}

 

#question 19

public class question19 {
    public static void main(String[] args) {
        int arr[] = {-10,-20,-11,-2,-5};

       int maxNum = arr[0];

       for(int i= 1; i<arr.length; i++){
        if(arr[i]> maxNum)
          maxNum = arr[i];
       }
       System.out.println(maxNum);
    }
}




#question 20

public class question20 {
    public static void main(String[] args) {
        int arr[][] = new int[4][5];

        for(int i = 0; i<4;i++){
           for(int j = 0; j<arr[0].length;j++)
           {
              arr[i][j] =2*(i+1)*(j+1);  // just a random calculation to get only even number

           }
        }

        for(int i=0;i<4;i++){  
            for(int j=0;j<5;j++){  
              System.out.print(arr[i][j]+" ");  
            }  
            System.out.println();  
           } 
    }
}


